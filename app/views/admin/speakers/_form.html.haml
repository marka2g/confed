= form_for [:admin, @speaker] do |f|
  %fieldset
    = render 'shared/error_messages', :target => @speaker
    %ul
      %li
        = f.label :name
        = f.text_field :name
      %li
        = f.label :title
        = f.text_field :title
      %li
        = f.label :location
        = f.text_field :location
      %li
        = f.label :bio
        = f.text_field :bio
      %li
        = f.label :company_name
        = f.text_field :company_name
      %li
        = f.label :company_url
        = f.text_field :company_url
      %li
        = f.label :email
        = f.text_field :email
      %li
        = f.label :twitter
        = f.text_field :twitter
      %li
        = f.label :website
        = f.text_field :website
      %li
        = f.label :speakerrate_url
        = f.text_field :speakerrate_url
    %p.actions
      = f.submit
      =# link_to 'Show', @presentation - not working
      \|
      = link_to 'Cancel & Go Back', admin_speakers_path